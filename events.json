---
layout: null
---
[
{%- assign now_time = site.time | date: "%s" | plus: 0 -%}
{% for event_hash in site.data.events %}
{%- assign event = event_hash[1] -%}
{%- assign event_time = event.dateIso | date:"%s" | plus: 0 -%}
{%- if event_time < now_time -%}
  {%- continue -%}
{%- endif -%}
  {
    "Id": "{{ event.id }}",
    "Href": "{{ event.link }}",
    "Title": "{{ event.title | replace: '"', '\\"' | strip }}",
    "Start": "{{ event.dateIso | date:"%Y-%m-%dT%T" }}",
    "Img": "{{ site.site }}/{{ site.baseurl }}/{{ event.localImg }}",
    "Group": "{{ event.community | replace: '"', '\\"' | strip }}",
    "Venue": "{{ event.place | replace: '"', '\\"' | strip }}",
    "VenueAddr": "{{ event.placeAddr | replace: '"', '\\"' | strip }}",
    "HtmlDescription": "{{ event.description | replace: '"', '\\"' | strip }}"
  }{% unless forloop.last %},{% endunlessÂ %}
{% endfor %}
]